.rotatRight{
    transform: scale(-1,1);
}
.tab{
	width:100%;
	display:table;
	position:absolute;
	margin:-6px 0 0;
}
.tab-c{
	display:table-cell;
	vertical-align:middle;
	text-align:center;
}
.box{
	max-height:450px;
	overflow:hidden;
}
.box-scroll{
	max-height:450px;
	overflow-y:scroll;
	overflow-x:hidden;
	width:-webkit-calc(100% + 25px);
	width:calc(100% + 25px);
}
.body{
	height:100%;
	display:flex;
	align-items:center;
}